//function declaration
function sumOf2plus2(){
    console.log (2+2)
};
sumOf2plus2()

//function expression
const twoplustwo = function sumOf2plus2(){
    console.log (2+2)
};
twoplustwo()

//reference
life = 100;
console.log (life-20)

